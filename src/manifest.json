{
  "manifest_version": 2,
  "name": "extension with vue",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.co.jp/*"
      ],
      "js": [
        "js/vue.js",
        "js/main.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://www.google.co.jp/*"
  ]
}
